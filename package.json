{
  "name": "localeyz-directus-enterprise-local",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "directus start",
    "start": "directus start",
    "build": "cd ./custom/bundles/localeyz/ && npm install && npm run build",
    "directus:schema:apply": "npx directus schema apply schema/schema.yaml",
    "directus:schema:snapshot": "npx directus schema snapshot --yes schema/schema.yaml && mkdir -p schema/previous && cp schema/schema.yaml schema/previous/$(date +'%Y%m%d%H%M%S')-schema.yaml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "directus": "^10.8.2",
    "directus-extension-localeyz": "./custom/bundles/localeyz/",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.8.0",
    "podcast-feed-parser": "^1.0.4",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-commit-msg-linter": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  }
